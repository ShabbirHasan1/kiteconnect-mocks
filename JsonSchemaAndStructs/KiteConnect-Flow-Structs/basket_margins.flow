export type BasketMargins = {
    $ref:        string;
    $schema:     string;
    definitions: Definitions;
};

export type Definitions = {
    BasketMargins: BasketMarginsClass;
    Data:          DataClass;
    Final:         Final;
    Pnl:           Pnl;
};

export type BasketMarginsClass = {
    additionalProperties: boolean;
    properties:           BasketMarginsProperties;
    required:             string[];
    title:                string;
    type:                 string;
};

export type BasketMarginsProperties = {
    data:   Data;
    status: Status;
};

export type Data = {
    $ref: string;
};

export type Status = {
    type: Type;
};

export type Type =
      "integer"
    | "number"
    | "string";

export type DataClass = {
    additionalProperties: boolean;
    properties:           DataProperties;
    required:             string[];
    title:                string;
    type:                 string;
};

export type DataProperties = {
    final:   Data;
    initial: Data;
    orders:  Orders;
};

export type Orders = {
    items: Data;
    type:  string;
};

export type Final = {
    additionalProperties: boolean;
    properties:           FinalProperties;
    required:             string[];
    title:                string;
    type:                 string;
};

export type FinalProperties = {
    additional:     Status;
    bo:             Status;
    cash:           Status;
    exchange:       Status;
    exposure:       Status;
    option_premium: Status;
    pnl:            Data;
    span:           Status;
    total:          Status;
    tradingsymbol:  Status;
    type:           Status;
    var:            Status;
};

export type Pnl = {
    additionalProperties: boolean;
    properties:           PnlProperties;
    required:             string[];
